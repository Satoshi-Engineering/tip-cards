{
  "name": "lightning-tip-cards",
  "version": "3.0.0",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "typecheck": "npm run shared-typecheck && npm run backend-typecheck && npm run frontend-typecheck",
    "test-units": "npm run backend-test-units && npm run frontend-test-units",
    "shared-build": "tsc -p ./shared/tsconfig.json",
    "shared-typecheck": "tsc -p ./shared/tsconfig.json --noEmit",
    "frontend-dev": "cd frontend && vite",
    "frontend-preview": "cd frontend && vite preview --port 5050",
    "frontend-build": "cd frontend && vite build",
    "frontend-typecheck": "vue-tsc -p ./frontend/tsconfig.json --noEmit",
    "frontend-lint": "eslint -c ./frontend/.eslintrc.js ./frontend --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "frontend-test-units": "jest --config ./frontend/jest.config.js ./frontend/tests/units",
    "frontend-test-integration": "jest --config ./frontend/jest.config.js ./frontend/tests/integration",
    "backend-cli": "cd backend && ts-node -r tsconfig-paths/register ./cli.ts",
    "backend-dev": "cd backend && nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./index.ts",
    "backend-debug": "cd backend && ts-node -r tsconfig-paths/register ./index.ts",
    "backend-build": "npm run shared-build && tsc -p ./backend/tsconfig.build.json",
    "backend-typecheck": "npm run shared-build && tsc -p ./backend/tsconfig.json --noEmit",
    "backend-tmp-dev": "cd backend && nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./tmpDevelop.ts",
    "backend-test-units": "jest --config ./backend/jest.config.js ./backend/tests/units",
    "backend-test-units-changes": "jest --config ./backend/jest.config.js --onlyChanged --watch ./backend/tests/units",
    "backend-test-integration": "cd backend && jest --config ./jest.config.js ./tests/integration",
    "backend-test-integration-changes": "cd backend && jest --config ./jest.config.js --onlyChanged --watch ./tests/integration",
    "backend-proxy": "cd backend && ts-node -r tsconfig-paths/register ./proxy.ts",
    "backend-ngrok": "cd backend && ts-node -r tsconfig-paths/register ./ngrok.ts",
    "drizzle-update-schema": "ts-node scripts/createSchemaFiles/script.ts",
    "drizzle-create-migration-files": "drizzle-kit generate:mysql --config=backend/drizzle.config.ts",
    "drizzle-migrate": "ts-node -P ./backend/tsconfig.json -r tsconfig-paths/register ./backend/drizzle.migrate.ts"
  },
  "dependencies": {
    "@node-redis/json": "^1.0.2",
    "@node-redis/search": "^1.0.5",
    "@popperjs/core": "^2.11.6",
    "@trpc/client": "^10.38.5",
    "@trpc/server": "^10.40.0",
    "axios": "^1.3.4",
    "bech32": "^2.0.0",
    "body-parser": "^1.20.0",
    "bootstrap-icons": "^1.10.3",
    "buffer": "^6.0.3",
    "canvg": "^4.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "drizzle-orm": "^0.29.0",
    "express": "^4.19.2",
    "express-http-proxy": "^1.6.3",
    "file-saver": "^2.0.5",
    "helmet": "^6.0.0",
    "jose": "^4.11.1",
    "jszip": "^3.10.1",
    "lnurl": "^0.25.1",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "luxon": "^3.3.0",
    "mysql2": "^3.6.3",
    "node-cron": "^3.0.2",
    "pinia": "^2.0.33",
    "qrcode-svg": "^1.1.0",
    "redis": "^4.3.1",
    "sanitize-html": "^2.10.0",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.6.1",
    "superjson": "^1.13.3",
    "tsconfig-paths": "^4.2.0",
    "vite-plugin-html": "^3.2.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@dbml/core": "^2.6.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/chromedriver": "^81.0.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.21",
    "@types/express-http-proxy": "^1.6.3",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.4.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.throttle": "^4.1.7",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.7.15",
    "@types/node-cron": "^3.0.8",
    "@types/qrcode-svg": "^1.1.1",
    "@types/sanitize-html": "^2.9.0",
    "@types/secp256k1": "^4.0.6",
    "@types/selenium-webdriver": "^4.1.13",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "bech32": "^2.0.0",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "chromedriver": "^119.0.0",
    "dotenv": "^16.0.3",
    "drizzle-kit": "^0.20.2",
    "eslint": "^8.36.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.21",
    "secp256k1": "^5.0.0",
    "selenium-webdriver": "^4.8.1",
    "tailwindcss": "^3.2.7",
    "tiny-secp256k1": "^2.2.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  },
  "overrides": {
    "express": "^4.19.2",
    "optionator": "^0.9.3",
    "semver": "^7.5.3"
  }
}
