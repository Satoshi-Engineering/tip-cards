{
  "name": "lightning-tip-cards",
  "version": "3.0.0",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "typecheck": "npm run shared-typecheck && npm run backend-typecheck && npm run frontend-typecheck",
    "test-units": "npm run shared-test-units && npm run backend-test-units && npm run frontend-test-units",
    "shared-build": "tsc -p ./shared/tsconfig.json",
    "shared-typecheck": "tsc -p ./shared/tsconfig.json --noEmit",
    "shared-test-units": "vitest run --config ./shared/vitest.config.mts ./shared/tests/units",
    "frontend-dev": "cd frontend && vite",
    "frontend-preview": "cd frontend && vite preview --port 5050",
    "frontend-build": "cd frontend && vite build",
    "frontend-typecheck": "cd frontend && vue-tsc --build --force",
    "frontend-lint": "eslint -c ./frontend/.eslintrc.js ./frontend --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "frontend-test-units": "vitest run --config ./frontend/vitest.config.ts ./frontend/tests/units",
    "frontend-test-integration": "start-server-and-test frontend-preview http://localhost:5050 'cypress run --e2e -C ./frontend/cypress.config.ts'",
    "backend-cli": "cd backend && ts-node -r tsconfig-paths/register ./cli.ts",
    "backend-dev": "cd backend && nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./index.ts",
    "backend-debug": "cd backend && ts-node -r tsconfig-paths/register ./index.ts",
    "backend-build": "npm run shared-build && tsc -p ./backend/tsconfig.build.json",
    "backend-typecheck": "npm run shared-build && tsc -p ./backend/tsconfig.json --noEmit",
    "backend-tmp-dev": "cd backend && nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./tmpDevelop.ts",
    "backend-test-units": "vitest run --config ./backend/vitest.config.ts ./backend/tests/units",
    "backend-test-integration": "vitest run --config ./backend/vitest.config.ts ./backend/tests/integration",
    "backend-proxy": "cd backend && ts-node -r tsconfig-paths/register ./proxy.ts",
    "backend-ngrok": "cd backend && ts-node -r tsconfig-paths/register ./ngrok.ts",
    "drizzle-update-schema": "ts-node scripts/createSchemaFiles/script.ts",
    "drizzle-create-migration-files": "drizzle-kit generate --config=backend/drizzle.config.ts",
    "drizzle-migrate": "ts-node -P ./backend/tsconfig.json -r tsconfig-paths/register ./backend/drizzle.migrate.ts"
  },
  "dependencies": {
    "@node-redis/json": "^1.0.2",
    "@node-redis/search": "^1.0.5",
    "@popperjs/core": "^2.11.8",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "axios": "^1.7.2",
    "bech32": "^2.0.0",
    "body-parser": "^1.20.2",
    "bootstrap-icons": "^1.11.3",
    "buffer": "^6.0.3",
    "canvg": "^4.0.2",
    "console-hooks": "^1.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "express": "^4.19.2",
    "express-http-proxy": "^2.0.0",
    "file-saver": "^2.0.5",
    "helmet": "^7.1.0",
    "jose": "^5.6.3",
    "jszip": "^3.10.1",
    "lnurl": "^0.26.2",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "luxon": "^3.4.4",
    "node-cron": "^3.0.3",
    "pinia": "^2.1.7",
    "postgres": "^3.4.4",
    "qrcode-svg": "^1.1.0",
    "redis": "^4.6.15",
    "sanitize-html": "^2.13.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "superjson": "^1.13.3",
    "telegram-sender": "^1.0.0",
    "tsconfig-paths": "^4.2.0",
    "vite-plugin-html": "^3.2.2",
    "vue": "^3.4.31",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@dbml/core": "^3.5.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-http-proxy": "^1.6.6",
    "@types/file-saver": "^2.0.7",
    "@types/jsdom": "^21.1.7",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.isequal": "^4.5.8",
    "@types/lodash.throttle": "^4.1.9",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.10",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode-svg": "^1.1.4",
    "@types/sanitize-html": "^2.11.0",
    "@types/secp256k1": "^4.0.6",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "bech32": "^2.0.0",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "cypress": "^13.13.0",
    "drizzle-kit": "^0.22.8",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^24.1.0",
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.39",
    "secp256k1": "^5.0.0",
    "start-server-and-test": "^2.0.4",
    "tailwindcss": "^3.4.4",
    "tiny-secp256k1": "^2.2.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.3",
    "vite": "^5.3.3",
    "vitest": "^2.0.0",
    "vue-tsc": "^2.0.26"
  }
}
